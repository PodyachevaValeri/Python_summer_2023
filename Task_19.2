class Fib:
    def __init__(self,c):
        self.a = 0
        self.b = 1
        self.c = c
    def __next__(self):
        self.c = self.b
        self.b = self.a + self.b
        self.a = self.c
        return self.c
f_iter = Fib()
print(next(f_iter))
print(next(f_iter))
